<h1>Registered Users</h1>

<table border="1">
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>

    <% users.forEach(function(user) { %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.first %></td>
            <td><%= user.last %></td>
            <td><%= user.email %></td>
        </tr>
    <% }); %>
</table>
